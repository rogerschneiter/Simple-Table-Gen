                                                <ComboBox fx:id="selectTableDelete" prefHeight="31.0" prefWidth="160.0" promptText="Select Table" />
                                                <Button mnemonicParsing="false" onAction="#delete" prefWidth="70.0" text="Delete">
                                                    <HBox.margin>
                                                        <Insets />
                                                    </HBox.margin>
                                                </Button>
                                                <Button mnemonicParsing="false" onAction="#deleteAll" prefWidth="70.0" text="Reset" />
                                            </children>
                                        </HBox>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab text="EXPORT" onSelectionChanged="#deactivateTabs">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
                                            fx:id="tabExport">
                                    <children>
                                        <HBox alignment="CENTER_LEFT" prefHeight="111.0" prefWidth="748.0" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                                            <children>
                                                <ComboBox fx:id="tableToExport" prefHeight="31.0" prefWidth="160.0" promptText="Select Table" />
                                                <ComboBox fx:id="wayOfExport" prefHeight="31.0" prefWidth="160.0" promptText="Select Export" />
                                                <Button mnemonicParsing="false" onAction="#export" prefWidth="70.0" text="Export">
                                                    <HBox.margin>
                                                        <Insets />
                                                    </HBox.margin>
                                                </Button>
                                                <Button mnemonicParsing="false" onAction="#exportAll" text="Export All" />
                                            </children>
                                        </HBox>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab text="INSERT" onSelectionChanged="#deactivateTabs">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
                                            fx:id="tabInsert">
                                    <children>
                                        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="111.0"
                                              prefWidth="748.0" spacing="10.0" AnchorPane.bottomAnchor="10.0"
                                              AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                                              AnchorPane.topAnchor="10.0">
                                            <children>
                                                <Button mnemonicParsing="false" onAction="#createTable"
                                                        text="New Insert"/>
                                                <Label text="Currently only works for tables without foreign keys!"/>
                                            </children>
                                        </HBox>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children></AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
